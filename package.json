{
  "name": "ganache-cli",
  "version": "6.12.2",
  "bin": {
    "ganache-cli": "cli.js"
  },
  "main": "./build/ganache-core.node.cli.js",
  "scripts": {
    "start": "node cli.js",
    "start-dev": "node cli.js -l 2000000000",
    "start-tar": "node cli.js --db_tar devchain.tar.gz",
    "build": "webpack-cli --config ./webpack/webpack.node.config.js",
    "docker": "docker build -t trufflesuite/ganache-cli . && docker run -p 8545:8545 trufflesuite/ganache-cli",
    "otp": "node ./scripts/generate-otp.js",
    "releaseNotes": "npx semantic-release --dry-run"
  },
  "dependencies": {
    "ethereumjs-util": "6.2.1",
    "source-map-support": "0.5.12",
    "yargs": "13.2.4"
  },
  "bundleDependencies": [
    "source-map-support",
    "yargs",
    "ethereumjs-util"
  ],
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "ganache-core": "2.13.2",
    "js-scrypt": "0.2.0",
    "node-loader": "^0.6.0",
    "notp": "2.0.3",
    "on-build-webpack": "^0.1.0",
    "prepend-file": "^1.3.1",
    "semantic-release": "^17.1.1",
    "scrypt": "6.0.3",
    "sha3": "1.2.2",
    "targz": "^1.0.1",
    "tmp": "^0.1.0",
    "fs-extra": "^5.0.0",
    "death": "^1.1.0",
    "shebang-loader": "0.0.1",
    "thirty-two": "1.0.2",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12"
  },
  "resolutions": {
    "**/ethereumjs-vm": "https://github.com/celo-org/ethereumjs-vm.git#21bb081"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/celo-org/ganache-cli"
  },
  "license": "MIT",
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "master",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/github",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "package-lock.json",
            "npm-shrinkwrap.json"
          ],
          "message": "${nextRelease.version}\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/release-notes-generator"
    ]
  }
}
