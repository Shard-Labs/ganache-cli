    "source-map-support",
    "yargs",
    "ethereumjs-util"
  ],
  "devDependencies": {
    "@celo/ganache-core": "../ganache-core",
    "@semantic-release/git": "^9.0.0",
    "js-scrypt": "0.2.0",
    "node-loader": "^0.6.0",
    "notp": "2.0.3",
    "on-build-webpack": "^0.1.0",
    "prepend-file": "^1.3.1",
    "scrypt": "6.0.3",
    "semantic-release": "^17.1.1",
    "sha3": "1.2.2",
    "targz": "^1.0.1",
    "tmp": "^0.1.0",
